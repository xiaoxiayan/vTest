#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/ppp/Documents/my/vTest/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="$NODE_PATH:/Users/ppp/Documents/my/vTest/node_modules/.pnpm/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../10.107.103.115+8001+vite@4.3.9_@types+node@20.3.2/node_modules/vite/bin/vite.js" "$@"
else
  exec node  "$basedir/../../../../../10.107.103.115+8001+vite@4.3.9_@types+node@20.3.2/node_modules/vite/bin/vite.js" "$@"
fi
